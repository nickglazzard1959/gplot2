LOGIN GUEST GUEST
SET_WORKING_CATALOG $USER

"**************************************"
"** BUILD SUPPORT LIBRARY FOR NOS/VE **"
"**************************************"

"--- SOURCE OF SUPPORT LIBRARY IS INLINE HERE."
COLLECT_TEXT OUTPUT=FT_VESUPPORT
      SUBROUTINE CNSTDIN( IUNIT )
C----
C     CONNECT IUNIT TO $INPUT ON NOS/VE.
C----
      INTEGER IUNIT
C
      OPEN(UNIT=IUNIT, FILE='$INPUT', STATUS='OLD')
      RETURN
      END
C
      SUBROUTINE CNSTDOU( IUNIT )
C----
C     CONNECT IUNIT TO $OUTPUT ON NOS/VE.
C----
      INTEGER IUNIT
C
      OPEN(UNIT=IUNIT, FILE='$OUTPUT', STATUS='OLD')
      RETURN
      END
C
      INTEGER FUNCTION IAND( IA, IB )
C----
C     WRAPPER FOR AND() ON NOS/VE
C----
      INTEGER IA, IB
      IAND = AND(IA, IB)
      RETURN
      END
C
      INTEGER FUNCTION ISHFT( IV, IS )
C----
C     WRAPPER FOR SHIFT() ON NOS/VE
C----
      INTEGER IV, IS
      ISHFT = SHIFT( IV, IS )
      RETURN
      END
C
      REAL FUNCTION E10VE( V )
C----
C     CALCULATE 10**V.
C     FOR UNKNOWN REASONS, 10**V AS SUCH IS NOT COMPUTED CORRECTLY
C     IN EITHER FORTRAN 1 OR FORTRAN 2 ON NOS/VE.
C     WHICH IS WORRYING, TO SAY THE LEAST!
C     THIS IS A WORKAROUND USED ONLY ON NOS/VE IN DIMFILM CODE.
C----
      REAL V
      E10VE = EXP( 2.302585093 * V )
      RETURN
      END
**

"--- COMPILE AND CREATE AN OBJECT LIBRARY."
FORTRAN INPUT=FT_VESUPPORT B=OBJ_VESUPPORT OPT=DEBUG DA=DT
CREATE_OBJECT_LIBRARY
ADD_MODULES LIBRARY=OBJ_VESUPPORT
GENERATE_LIBRARY LIBRARY=LIB_VESUPPORT
QUIT
DISPLAY_OBJECT_LIBRARY LIBRARY=LIB_VESUPPORT
