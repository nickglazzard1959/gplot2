LOGIN GUEST GUEST
SET_WORKING_CATALOG $USER

"**************************************"
"** BUILD SUPPORT LIBRARY FOR NOS/VE **"
"**************************************"

"--- SOURCE OF SUPPORT LIBRARY IS INLINE HERE."
COLLECT_TEXT OUTPUT=FT_VESUPPORT
      SUBROUTINE CNSTDIN( IUNIT )
C----
C     CONNECT IUNIT TO $INPUT ON NOS/VE.
C     THIS MAYBE WRONG. PERHAPS CONNEC SHOULD BE USED ON NOS/VE
C     AS IT IS AVAILABLE THERE.
C----
      INTEGER IUNIT
C
C      OPEN(UNIT=IUNIT, FILE='$INPUT', STATUS='OLD')
      RETURN
      END
C
      INTEGER FUNCTION IAND( IA, IB )
C----
C     WRAPPER FOR AND() ON NOS/VE
C----
      INTEGER IA, IB
      IAND = AND(IA, IB)
      RETURN
      END
C
      INTEGER FUNCTION ISHFT( IV, IS )
C----
C     WRAPPER FOR SHIFT() ON NOS/VE
C----
      INTEGER IV, IS
      ISHFT = SHIFT( IV, IS )
      RETURN
      END
C
      SUBROUTINE SNDBYT( DATA, NDATA )
C----
C     OUTPUT NDATA BYTES FROM DATA TO TERMINAL.
C     NO IDEA HOW TO REALLY DO THAT YET.
C----
      INTEGER NDATA
      INTEGER DATA(NDATA)
C
      RETURN
      END
**

"--- COMPILE AND CREATE AN OBJECT LIBRARY."
FORTRAN INPUT=FT_VESUPPORT B=OBJ_VESUPPORT OPT=DEBUG
CREATE_OBJECT_LIBRARY
ADD_MODULES LIBRARY=OBJ_VESUPPORT
GENERATE_LIBRARY LIBRARY=LIB_VESUPPORT
QUIT
DISPLAY_OBJECT_LIBRARY LIBRARY=LIB_VESUPPORT
