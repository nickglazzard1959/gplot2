LOGIN GUEST GUEST
CHANGE_LINK_ATTRIBUTES USER=GUEST PASSWORD=GUEST
SET_WORKING_CATALOG $USER

DISPLAY_JOB_LIMIT

CREATE_VARIABLE NAME=ERRS KIND=STATUS
CREATE_CATALOG OBEY_FILES STATUS=ERRS
SET_WORKING_CATALOG $USER.OBEY_FILES

