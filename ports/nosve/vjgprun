LOGIN GUEST GUEST
CHANGE_LINK_ATTRIBUTES USER=GUEST PASSWORD=GUEST
SET_WORKING_CATALOG $USER

CREATE_FILE_CONNECTION SF=$OUTPUT F=TAPE6

COLLECT_TEXT OUTPUT=TAPE7
DEV EPSCOL EPSA
MOVE 0.2 0.2
DRAW 0.8 0.8
EXIT
**
  
EXET F=OBJ_GPLOT L=(LIB_DIMFM,LIB_GRDEV,LIB_UTILS,LIB_VESUPPORT) ..
  PRESET_VALUE=ZERO

DISC
DISC $LOCAL

REPLACE_FILE FROM=EPSA001_EPS TO=EPSA001 DC=A6

COLLECT_TEXT OUTPUT=TAPE7
DEV SVG SVGA
MOVE 0.2 0.2
DRAW 0.8 0.8
VERSION
EVAL 1,5,/,P
EXIT
**
  
EXET F=OBJ_GPLOT L=(LIB_DIMFM,LIB_GRDEV,LIB_UTILS,LIB_VESUPPORT) ..
  PRESET_VALUE=ZERO

DISC
DISC $LOCAL

REPLACE_FILE FROM=SVGA001_SVG TO=SVGA001 DC=A6

GET_FILE FROM=OBMYSTY TO=OBMYSTY DATA_CONVERSION=A6

COLLECT_TEXT OUTPUT=TAPE7
DEV SVG SVGA
CL G1FM
OB OBMYSTY
EXIT
**
  
EXET F=OBJ_GPLOT L=(LIB_DIMFM,LIB_GRDEV,LIB_UTILS,LIB_VESUPPORT)

DISC
DISC $LOCAL

REPLACE_FILE FROM=G1FM001_SVG TO=G1FM001 DC=A6
