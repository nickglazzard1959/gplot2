LOGIN GUEST GUEST
CHANGE_LINK_ATTRIBUTES USER=GUEST PASSWORD=GUEST
SET_WORKING_CATALOG $USER

COLLECT_TEXT OUTPUT=TAPE7
DEV EPSCOL EPSA
MOVE 0.2 0.2
DRAW 0.8 0.8
EXIT
**
  
EXET F=OBJ_GPLOT L=(LIB_DIMFM,LIB_GRDEV,LIB_UTILS,LIB_VESUPPORT)

DISC
DISC $LOCAL

REPLACE_FILE FROM=EPSA001_EPS TO=EPSA001 DC=A6

COLLECT_TEXT OUTPUT=TAPE7
DEV SVG SVGA
MOVE 0.2 0.2
DRAW 0.8 0.8
EXIT
**
  
EXET F=OBJ_GPLOT L=(LIB_DIMFM,LIB_GRDEV,LIB_UTILS,LIB_VESUPPORT)

DISC
DISC $LOCAL

REPLACE_FILE FROM=SVGA001_SVG TO=SVGA001 DC=A6
