*DECK DFX215 F
      SUBROUTINE DFX215(THETA)
C
C    **INTERNAL SYMANG EQUIVALENT FUNCTION**
C
*CALL,DFXC01
*CALL,DFXC00
*CALL,DFXC03
C    ANGLS IS PERMANENTLY SAVED ANGLE - ANGL IS FOR CURRENT TRANSFORM
      ANGLS = THETA*ANGCON(1,ANGRP(1))
      ANGL = ANGLS
      IF (.NOT.HS) ANGL = ANGL + ALPHA
      CANGL = COS(ANGL)
      SANGL = SIN(ANGL)
      RETURN
      END
