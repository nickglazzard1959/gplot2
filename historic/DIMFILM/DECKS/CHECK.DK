*DECK CHECK F
      SUBROUTINE CHECK(I)
*CALL,DFXC00
*CALL,DFXC05A
      ROUTIN = 'CHECK'
*CALL,RIN
      I1 = I
      IF (IABS(I).LE.3) GO TO 2
      I1 = ISIGN(3,I)
      IF (ICHECK.GT.0) WRITE(ERRREC,1) I, I1
*CALL,LINOUT0
    1 FORMAT(1H0,'**DIMFILM WARNING**  ILLEGAL PARAMETER IN CALL TO CHEC
     1K'/1H ,21X,'PARAMETER WAS ',I6,' DEFAULT OF ',I6,' USED')
    2 ICHECK = I1
*CALL,ROUT
      RETURN
      END
