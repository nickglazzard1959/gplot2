*DECK CMSMS3 F
      SUBROUTINE DFXMS3(USERID)
      CHARACTER*(*) USERID
C    RETURNS CMS USERID
C    IF INTERACTIVE ACQUIRES SYSTEM USERID, ELSE
C    IF BATCH MAKES APPROPRIATE SYSTEM INFO CALL OF VMBATCH
C
C    CALLED AS    CALL DFXMS3(JOBNAM)
C                 CHARACTER*8 JOBNAM
C
*CALL ,DFXC12
      IF (SYSIA) THEN
           CALL DFXMSJ(USERID)
      ELSE
           CALL DFXMSB('ACCOUNT ',USERID,LENRET,IERR)
      ENDIF
      RETURN
      END
