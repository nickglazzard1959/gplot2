*DECK DFX145 F
      REAL FUNCTION DFX145(N)
C    RETURNS MAXIMUM POSSIBLE INTENSITY RETAINING COLOUR
*CALL,DFXC04
C    ALGORITHM SHOULD FUNCTION CORRECTLY FOR ANY METHOD OF
C    INTENSITY CALCULATION DEPENDENT ON LINEAR COMBINATION
C    OF COLOUR TRIPLET ELEMENTS
      F = 0.0
      Z = AMAX1(R0(N),G0(N),B0(N))
      IF (Z.GT.0.0) F = 1./Z
      DFX145 = F*ZINT0(N)
      RETURN
      END
