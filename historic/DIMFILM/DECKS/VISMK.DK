*DECK VISMK F
      SUBROUTINE VISMK(I)
*CALL,DFXC00
*CALL,DFXC05A
      ROUTIN = 'VISMK'
*CALL,RIN
      IF (I.LT.1.OR.I.GT.3) THEN
                  IF (ICHECK.GE.1) WRITE(ERRREC,11) I,MKVIS
   11 FORMAT(1H0,'**DIMFILM WARNING** VISMK CALLED WITH INVALID PARAMETE
     1R ',I3,' CURRENT VALUE ',I1,' RETAINED')
*CALL,LINOUT0
      ELSE
                  MKVIS = I
      ENDIF
*CALL,ROUT
      RETURN
      END
