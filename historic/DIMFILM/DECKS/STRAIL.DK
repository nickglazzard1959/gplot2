*DECK STRAIL F
      SUBROUTINE STRAIL(ITEXT,NA,SLONG)
      CHARACTER*(*) ITEXT
C    RETURNS LENGTH OF STRING (AT UNIT HEIGHT)
C    WITH TRAILING BLANKS STRIPPED
C    SUBSEQUENT CALL TO SYMXT SHOULD BE (ITEXT(1:NA))
*CALL,DFXC05A
      ROUTIN = 'STRAIL'
*CALL,RIN
      CALL DFX213(ITEXT,NA,SLONG)
*CALL,ROUT
      RETURN
      END
