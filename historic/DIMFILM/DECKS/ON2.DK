*DECK ON2 F
      SUBROUTINE ON2(X1,Y1)
*CALL,DFXC00
*CALL,DFXC05A
      ROUTIN = 'ON2'
*CALL,RIN
      CALL DFX106(X1,Y1)
      IF (INTEND.LT.0) GO TO 1
      INTCH = INTCH + 1
      IF (INTCH.LT.INTEND) GO TO 1
C    N.B. BROKEN LINE PATTERN IS PRESERVED ACROSS INTERUPT SYMBOLS
C    INTERNALLY IN MARKER/DFX200
      CALL DFX200(IICHAR)
      INTCH = 0
    1 CONTINUE
*CALL,IMM
*CALL,ROUT
      RETURN
      END
