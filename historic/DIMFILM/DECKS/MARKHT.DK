*DECK MARKHT F
      SUBROUTINE MARKHT(HEIGHT)
*CALL,DFXC01
*CALL,DFXC00
*CALL,DFXC05A
      ROUTIN = 'MARKHT'
*CALL,RIN
      IF (HEIGHT.GT.0.0) THEN
                  HTMK = HEIGHT
      ELSE
                  IF (ICHECK.GT.0) WRITE(ERRREC,1) HEIGHT,HTMK
*CALL,LINOUT0
    1 FORMAT(1H0,'**DIMFILM WARNING**  MARKHT CALLED WITH ILLEGAL ARGUME
     1NT ',1PE14.5/1H ,21X,'HEIGHT USED WAS ',1PE14.5)
      ENDIF
*CALL,ROUT
      RETURN
      END
