*DECK RADSEP F
      SUBROUTINE RADSEP(ANG)
*CALL,DFXC00
*CALL,DFXC02
*CALL,DFXC03
*CALL,DFXC05A
      ROUTIN = 'RADSEP'
*CALL,XIN
      A = ANG
      DEGPRT = A*ANGCON(1,ANGRP(2))
C    NEGATIVE IS AUTO CALCULATION
C    POSITIVE OR ZERO IS ACTUAL SPECIFIED SEPARATION
*CALL,XOUT
      RETURN
      END
