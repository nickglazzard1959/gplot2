*DECK CMY F
      SUBROUTINE CMY(C,M,Y)
      REAL C,M,Y
*CALL,DFXC04
*CALL,DFXC05A
      ROUTIN = 'CMY'
*CALL,RIN
      CALL DFX160(1,C,M,Y,RRR,GGG,BBB,IERR)
      IF (IERR.LE.0) THEN
                        DO 1 I=1,NRGB
    1                   CALL DFX143(I,RRR,GGG,BBB)
      ENDIF
*CALL,ROUT
      RETURN
      END
