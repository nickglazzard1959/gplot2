*DECK CVX01O F
      SUBROUTINE DFX01O(DN,ODN,BYTES)
      INTEGER DN,ODN,BYTES
      INTEGER*4 NW
      INTEGER*2 BUFF(1200)
C    BUFF MUST EXCEED LENGTH DECLARED IN MAIN DRIVERS
      CALL DFX01U(DN,ERR)
    1 CALL DFX01R(DN,BUFF,NW,ERR)
      IF (NW.EQ.0) GO TO 2
C    FOLLOWING WRITE IS TO A FORTRAN UNIT - MAY NEED TO CHANGE
C    IF SCRATCH FILE USED WHEN ACTUAL TAPES SUPPORTED
      WRITE(ODN) (BUFF(I),I=1,NW)
      BYTES = BYTES + 2*NW
      GOTO 1
    2 RETURN
      END
