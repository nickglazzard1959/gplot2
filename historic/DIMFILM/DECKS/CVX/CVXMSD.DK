*DECK CVXMSD F
      SUBROUTINE DFXMSD(DN,BUFF,NBYTS,ERR)
C    READ - FOR FONT DATA
C
C    WHEN IN FORTRAN MUST FIRST WRITE RECORD WITH
C    NUMBER OF WORDS WRITTEN, THEN RECORD WITH DATA
      IMPLICIT INTEGER (A-Z)
      INTEGER*4 DN
      INTEGER*2 BUFF(*)
      INTEGER*4 N16,NBYTS
      ERR = 0
      N16 = NBYTS/2
      READ (DN,END=1) (BUFF(I),I=1,N16)
      RETURN
    1 ERR = 12
      RETURN
      END
