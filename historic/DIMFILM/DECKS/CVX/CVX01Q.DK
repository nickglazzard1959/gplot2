*DECK CVX01Q F
      SUBROUTINE DFX01Q(BUFF,NW,CHKSUM)
C    COMPUTE CHECKSUM FOR NW WORDS IN BUFF
C
      IMPLICIT INTEGER (A-Z)
      INTEGER*2 BUFF(*),CHKSUM
      INTEGER*4 NW
      CHKSUM = BUFF(1)
      IF (NW.EQ.1) GO TO 2
      DO 1 I=2,NW
    1 CHKSUM = IEOR(CHKSUM,BUFF(I))
    2 RETURN
      END
