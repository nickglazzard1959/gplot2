*DECK CVXM00 F
      SUBROUTINE DFXM00(IERR)
C    TAKE APPROPRIATE TERMINATION/ABORT ACTION
C    IERR = 1 AFTER SUCCESSFUL COMPLETION OF DIMEND
C         = 2 FAILURE DURING DIMEND - FROM DFX002
C    CONVEX EXIT PASSES STATUS TO SYSTEM/USER
C    CONVEX EXIT STATUS IS SYSTEM DEPENDENT
      IF (IERR.EQ.1) CALL EXIT(16)
      CALL EXIT(17)
C    (CONVEX ABORT TERMINATES WITH MEMORY IMAGE)
      END
