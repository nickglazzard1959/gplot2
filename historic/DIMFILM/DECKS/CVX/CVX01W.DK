*DECK CVX01W F
      SUBROUTINE DFX01W(DN,BUFF,NW,ERR)
C    WRITE - FOR D48 DRIVERS
C
C    FORTAN VERSION
      IMPLICIT INTEGER (A-Z)
      INTEGER*4 DN
      INTEGER*2 BUFF(*)
      INTEGER*4 NW
      ERR = 0
      IF (NW.EQ.0) RETURN
      WRITE(DN) (BUFF(I),I=1,NW)
      RETURN
      END
