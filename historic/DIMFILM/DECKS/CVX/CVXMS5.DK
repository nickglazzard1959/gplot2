*DECK CVXMS5 F
      SUBROUTINE DFXMS5(SYSLEV)
C    RETURNS CURRENT CONVEX OPERATING SYSTEM LEVEL
C    (MAXIMUM 16 CHARACTERS)
C
C    USES DFXMSB (RETURNS MACHINE + SYSTEM VIA UNAME)
      CHARACTER*(*) SYSLEV
      CHARACTER*64 REL,VER
      INTEGER*4 LREL,LVER
      INTEGER*2 SERIAL
      CALL DFXMSB(REL,VER,LREL,LVER,SERIAL)
      SYSLEV = 'ConvexOS '//VER(1:LVER)
      RETURN
      END
