*DECK CVXMSA F
      SUBROUTINE DFXMSA(IERR)
C    SAVE SYSTEM ERROR RETURN CODE AS INTEGER AND CHARACTER STRING
C    (CONVERSION ACCORDING TO SYSTEM)
*CALL,DFXCBD
*CALL,DFXC05
C    FOR CONVEX ERRORS ARE DECIMAL INTEGERS
      SYSERR = IERR
      WRITE(SYSCOD,10) IERR
   10 FORMAT(I8)
      RETURN
      END
