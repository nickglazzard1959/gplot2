*DECK LINK F
      SUBROUTINE LINK(X,Y,N1)
      REAL X(N1), Y(N1)
      LOGICAL OUT
*CALL,DFXC00
*CALL,DFXC05A
*CALL,DFXCD0
      ROUTIN = 'LINK'
*CALL,RIN
      CALL DFX503(.TRUE.,X,Y,N1,OUT)
      IF (.NOT.OUT) GO TO 99
      IF (ICHECK.EQ.2) WRITE(ERRREC,20) ROUTIN
*CALL,LINOUT1
   20 FORMAT(1H0,'**DIMFILM WARNING**  OUT OF BOUNDS DETECTED DURING CAL
     1L OF ',A)
   99 CONTINUE
*CALL,IMM
*CALL,ROUT
      RETURN
      END
