*DECK DFX143 F
      SUBROUTINE DFX143(N,RR,GG,BB)
*CALL,DFXC00
*CALL,DFXC04
C    SELECT TRUE COLOUR MODE
      NLUT(N) = -1
C    NOW STORE COLOUR
      R0(N) = RR
      G0(N) = GG
      B0(N) = BB
      ZINT0(N) = DFX146(N)
C    NOW SET CURRENT COLOUR/INTENSITY TO BASE SET
      R(N) = R0(N)
      G(N) = G0(N)
      B(N) = B0(N)
      ZINT(N) = ZINT0(N)
      IF (ICOLPT(IRGBN).EQ.N) CALL DFX140(IRGBN)
      RETURN
      END
