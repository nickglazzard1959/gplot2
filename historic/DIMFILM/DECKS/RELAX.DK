*DECK RELAX F
      SUBROUTINE RELAX
C    ANGLES ALWAYS HELD INTERNALLY RELATIVE TO FRAME AND THIS VALUE
C    RETURNED VIA VAL FUNCTION
*CALL,DFXC01
*CALL,DFXC00
*CALL,DFXC05A
      ROUTIN = 'RELAX'
*CALL,XIN
      IF (HS) THEN
              HS = .FALSE.
              ANGL = ANGL + ALPHA
              CANGL = COS(ANGL)
              SANGL = SIN(ANGL)
      ENDIF
*CALL,XOUT
      RETURN
      END
