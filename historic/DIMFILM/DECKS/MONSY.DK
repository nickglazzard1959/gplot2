*DECK MONSY F
      SUBROUTINE MONSY(NUMMON)
*CALL,DFXC02
*CALL,DFXC00
*CALL,DFXC05A
      ROUTIN = 'MONSY'
*CALL,RIN
      IF (NUMMON.LT.2) THEN
             MONY = 0
             IF (ICHECK.GT.0) WRITE(ERRREC,2) NUMMON
*CALL,LINOUT0
    2 FORMAT(1H0,'**DIMFILM WARNING** INVALID ARGUMENT ',I10,
     1 ' IN CALL TO MONSY'/1H ,21X,'AUTOY WILL BE PERFORMED')
      ELSE
             MONY = NUMMON
      ENDIF
*CALL,ROUT
      RETURN
      END
