*DECK MONSX F
      SUBROUTINE MONSX(NUMMON)
*CALL,DFXC02
*CALL,DFXC00
*CALL,DFXC05A
      ROUTIN = 'MONSX'
*CALL,RIN
      IF (NUMMON.LT.2) THEN
             MONX = 0
             IF (ICHECK.GT.0) WRITE(ERRREC,2) NUMMON
*CALL,LINOUT0
    2 FORMAT(1H0,'**DIMFILM WARNING** INVALID ARGUMENT ',I10,
     1 ' IN CALL TO MONSX'/1H ,21X,'AUTOX WILL BE PERFORMED')
      ELSE
             MONX = NUMMON
      ENDIF
*CALL,ROUT
      RETURN
      END
