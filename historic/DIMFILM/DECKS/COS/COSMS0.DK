*DECK COSMS0 F
      SUBROUTINE DFXMS0(NA,TEXT)
C
C       PRINT A MESSAGE IN THE LOGFILE
*
*       DIMFILM MACHINE DEPENDENT INTERFACE - CRAY VERSION
*       VERSION 1.0        01/02/84        STEVE WISE,ULCC
*
      CHARACTER TEXT*(*) , MESS*72
      DATA MESS /'DFILM - '/
      MESS(9:)=TEXT
      MESS(72:72)=CHAR(0)
      CALL REMARK2(MESS)
      RETURN
      END
