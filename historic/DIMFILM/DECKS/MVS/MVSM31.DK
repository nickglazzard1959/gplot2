*DECK MVSM31 F
      SUBROUTINE DFXM31(IVAL)
C    N.B. REFERENCED WITH REAL - SO IVAL MUST HAVE SAME LENGTH
C         AS REAL
C
C    SETS LOW ORDER BIT
      IVAL = IBSET(IVAL,0)
      RETURN
      ENTRY DFXM32(IVAL)
C    CLEARS LOW ORDER BIT
      IVAL = IBCLR(IVAL,0)
      RETURN
      END
