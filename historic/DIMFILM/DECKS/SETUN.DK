*DECK SETUN F
      SUBROUTINE SETUN(UNY)
*CALL,DFXC00
*CALL,DFXC01
*CALL,DFXC05A
      ROUTIN = 'SETUN'
*CALL,RIN
      IF (UNY.LT.0.0) THEN
                  IF (ICHECK.GT.0) WRITE(ERRREC,100) UNY,UNYSP
*CALL,LINOUT0
  100 FORMAT(1H0,'**DIMFILM WARNING**  SETUN CALLED WITH INVALID SEPARAT
     1ION ',1PE16.8/1H ,21X,'CURRENT VALUE ',1PE16.8,' RETAINED')
      ELSE
                  UNYSP = UNY
      ENDIF
*CALL,ROUT
      RETURN
      END
