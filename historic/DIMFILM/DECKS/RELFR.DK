*DECK RELFR F
      SUBROUTINE RELFR
C    ANGLES ALWAYS HELD INTERNALLY RELATIVE TO FRAME AND THIS VALUE
C    RETURNED VIA VAL FUNCTION
*CALL,DFXC01
*CALL,DFXC00
*CALL,DFXC05A
      ROUTIN = 'RELFR'
*CALL,XIN
      IF (.NOT.HS) THEN
                   HS = .TRUE.
                   ANGL = ANGL - ALPHA
                   CANGL = COS(ANGL)
                   SANGL = SIN(ANGL)
      ENDIF
*CALL,XOUT
      RETURN
      END
