*DECK SETCEX F
      SUBROUTINE SETCEX(EX)
*CALL,DFXC00
*CALL,DFXC01
*CALL,DFXC05A
      ROUTIN = 'SETCEX'
*CALL,RIN
      IF (EX.LE.0.0) THEN
              IF (ICHECK.GT.0) WRITE(ERRREC,100) EX,CEXP
*CALL,LINOUT0
  100 FORMAT(1H0,'**DIMFILM WARNING**  SETCEX CALLED WITH INVALID EXPANS
     1ION FACTOR ',1PE16.8,' CURRENT VALUE ',E16.8,' RETAINED')
      ELSE
              CEXP = EX
      ENDIF
*CALL,ROUT
      RETURN
      END
